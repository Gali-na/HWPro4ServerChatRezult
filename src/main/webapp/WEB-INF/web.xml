        <servlet-name>WriteUser</servlet-name>

        <url-pattern>/writeUser</url-pattern>

    </servlet-mapping>

    <servlet>

        <servlet-name>TableUsers</servlet-name>

        <servlet-class>com.example.demoh.TableUsers</servlet-class>

    </servlet>


    <servlet-mapping>

        <servlet-name>TableUsers</servlet-name>

        <url-pattern>/tableUsers</url-pattern>

    </servlet-mapping>
    <servlet>

        <servlet-name>StopMessage</servlet-name>

        <servlet-class>com.example.demoh.StopMessage</servlet-class>

    </servlet>


    <servlet-mapping>

        <servlet-name>StopMessage</servlet-name>

        <url-pattern>/stopMessage</url-pattern>

    </servlet-mapping>
    <servlet>

        <servlet-name>PrivateChat</servlet-name>

        <servlet-class>com.example.demoh.PrivateChat</servlet-class>

    </servlet>


    <servlet-mapping>

        <servlet-name>PrivateChat</servlet-name>

        <url-pattern>/privateChat</url-pattern>

    </servlet-mapping>

    <servlet>

        <servlet-name>PrintingGeneralMessages</servlet-name>

        <servlet-class>com.example.demoh.PrintingGeneralMessages</servlet-class>

    </servlet>


    <servlet-mapping>

        <servlet-name>PrintingGeneralMessages</servlet-name>

        <url-pattern>/printingGeneralMessages</url-pattern>

    </servlet-mapping>

</web-app>