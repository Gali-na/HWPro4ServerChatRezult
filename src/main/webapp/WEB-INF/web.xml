<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>

        <servlet-name>AddMessage</servlet-name>

        <servlet-class>com.example.demoh.AddMessage</servlet-class>

    </servlet>


    <servlet-mapping>

        <servlet-name>AddMessage</servlet-name>

        <url-pattern>/add</url-pattern>

    </servlet-mapping>

    <servlet>

        <servlet-name>TableRegistration</servlet-name>

        <servlet-class>com.example.demoh.TableRegistration</servlet-class>

    </servlet>


    <servlet-mapping>

        <servlet-name>TableRegistration</servlet-name>

        <url-pattern>/TableRegis</url-pattern>

    </servlet-mapping>

    <servlet>

        <servlet-name>WriteUser</servlet-name>

        <servlet-class>com.example.demoh.WriteUser</servlet-class>

    </servlet>


